_________________________________________________P->Q=~Q->~P__________________________________
#include<iostream>

using namespace std;

int main(){

char P[4]={'T','T','F','F'};
char Q[4]={'T','F','T','F'};
cout<<"\n";
cout<<"Conditional"<<endl;
cout<<"P"<<'\t'<<"Q"<<'\t'<<"P->Q"<<endl;

for(int i=0;i<4;i++){

    if(P[i]=='T'&&Q[i]=='F')
        cout<<P[i]<<'\t'<<Q[i]<<'\t'<<'F'<<endl;
    else
        cout<<P[i]<<'\t'<<Q[i]<<'\t'<<'T'<<endl;
}

for(int i=0;i<4;i++){

    if(P[i]=='T'){
        P[i]='F';
    }
    else
        P[i]='T';


}
for(int i=0;i<4;i++){

    if(Q[i]=='T'){
        Q[i]='F';
    }
    else
        Q[i]='T';
}


cout<<"~Q"<<'\t'<<"~P"<<'\t'<<"~Q->~P__________________________________"<<endl;
for(int i=0;i<4;i++){

   if(P[i]=='T'&&Q[i]=='F')
        cout<<P[i]<<'\t'<<Q[i]<<'\t'<<'F'<<endl;
    else
        cout<<P[i]<<'\t'<<Q[i]<<'\t'<<'T'<<endl;
}


}
*/

_________________________________________________P<->Q = (P ^ Q) V (~P ^ ~Q ) __________________________________



/*
int main(){

char P[4]={'T','T','F','F'};
char Q[4]={'T','F','T','F'};


